{
    "version": 2,
    "rewrites": [
        {
             "source": "/(.*)", 
             "dest": "/" 
        }
    ],
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
} 